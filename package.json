{
  "name": "sms-blossom-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "node --watch src/server.js",
    "dev:worker": "node --watch src/worker.js",
    "start": "node src/server.js",
    "start:worker": "node src/worker.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "env:check": "node src/scripts/checkEnv.js",
    "prisma:validate": "prisma validate",
    "prisma:status": "prisma migrate status",
    "test": "vitest run",
    "test:watch": "vitest",
    "audit": "npm audit --audit-level=high",
    "depcheck": "depcheck --skip-missing true",
    "openapi:lint": "redocly lint openapi/openapi.yaml",
    "check": "npm run env:check && npm run lint && npm run format:check && npm run prisma:validate && npm run prisma:status && npm run test && npm run openapi:lint && npm run audit && npm run depcheck"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "bullmq": "^5.60.0",
    "cookie": "^1.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.8.0",
    "libphonenumber-js": "^1.11.7",
    "liquidjs": "^10.9.2",
    "nanoid": "^5.0.7",
    "pino": "^9.4.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.1",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@redocly/cli": "^2.3.0",
    "@types/supertest": "^6.0.3",
    "depcheck": "^1.4.7",
    "envalid": "^8.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "prettier": "^3.6.2",
    "prisma": "^5.20.0",
    "supertest": "^7.1.4",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
